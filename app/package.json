{
  "name": "app-desktop",
  "description": "Console UI experience for desktop",
  "version": "1.0.0",
  "main": "src/electron.js",
  "license": "MIT",
  "author": {
    "name": "kless",
    "email": "klessitom@gmail.com"
  },
  "scripts": {
    "build": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve",
    "app-start": "yarn electron ./src/electron.js",
    "make-and-start": "electron-forge package && electron-forge start",
    "make-linux": "electron-forge make -p linux",
    "make-win": "electron-forge make -p win32"
  },
  "dependencies": {
    "app-db": "file:../app-db",
    "app-node-layer": "file:../app-node-layer",
    "electron-packager": "^17.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.14.1",
    "styled-components": "6.0.8",
    "stylis": "4.3.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.4.2",
    "@electron-forge/maker-deb": "6.4.2",
    "@rabbitholesyndrome/electron-forge-maker-portable": "^0.2.0",
    "@trivago/prettier-plugin-sort-imports": "4.2.0",
    "@types/common": "file:../@types",
    "@types/node": "^20.8.3",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "@types/styled-components": "5.1.27",
    "electron": "25.2.0",
    "electron-reload": "2.0.0-alpha.1",
    "html-webpack-plugin": "5.5.0",
    "prettier": "2.7.0",
    "react-icons": "4.8.0",
    "ts-loader": "9.3.0",
    "tsconfig-paths-webpack-plugin": "4.0.0",
    "typescript": "5.0.4",
    "webpack": "5.73.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.9.2"
  },
  "config": {
    "forge": {
      "makers": [
        {
          "name": "@electron-forge/maker-deb"
        },
        {
          "name": "@rabbitholesyndrome/electron-forge-maker-portable"
        }
      ]
    }
  }
}
